<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".Index"
    android:id="@+id/program">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:id="@+id/imageButton_cityMap"
        android:src="@mipmap/city_map"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:onClick="goCityMap"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="City Map"
        android:id="@+id/textView_cityMap"
        android:textSize="12dp"
        android:textColor="#000000"
        android:typeface="normal"
        android:textStyle="bold"
        android:layout_below="@+id/imageButton_cityMap"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:id="@+id/imageButton_itinerary"
        android:src="@mipmap/itinerary"
        android:layout_above="@+id/textView_itinerary"
        android:layout_alignLeft="@+id/textView_itinerary"
        android:layout_alignStart="@+id/textView_itinerary"
        android:onClick="goItinerary"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Itinerary"
        android:id="@+id/textView_itinerary"
        android:textSize="12dp"
        android:textColor="#000000"
        android:typeface="normal"
        android:textStyle="bold"
        android:layout_above="@+id/imageButton_cityMap"
        android:layout_alignLeft="@+id/imageButton_cityMap"
        android:layout_alignStart="@+id/imageButton_cityMap"
        android:layout_marginBottom="20dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:id="@+id/imageButton_setting"
        android:src="@mipmap/setting"
        android:layout_below="@+id/textView_cityMap"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:onClick="goSetting"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:id="@+id/imageButton_event"
        android:src="@mipmap/event"
        android:layout_below="@+id/textView_itinerary"
        android:layout_toRightOf="@+id/imageButton_cityMap"
        android:layout_toEndOf="@+id/imageButton_cityMap"
        android:layout_marginLeft="20dp"
        android:onClick="goPostNotification"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Setting"
        android:id="@+id/textView_setting"
        android:textSize="12dp"
        android:textColor="#000000"
        android:typeface="normal"
        android:textStyle="bold"
        android:layout_below="@+id/imageButton_setting"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notification"
        android:id="@+id/textView_event"
        android:textSize="12sp"
        android:textColor="#000000"
        android:typeface="normal"
        android:textStyle="bold"
        android:layout_below="@+id/imageButton_event"
        android:layout_alignLeft="@+id/imageButton_event"
        android:layout_alignStart="@+id/imageButton_event"
        android:layout_marginLeft="-5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="IEEE ICWS 2015"
        android:id="@+id/textView5"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="June 27 - July 2, 2015"
        android:id="@+id/textView6"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="New York, USA"
        android:id="@+id/textView7"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:id="@+id/imageButton_venueMap"
        android:src="@mipmap/venue_map"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/imageButton_cityMap"
        android:layout_toStartOf="@+id/imageButton_cityMap"
        android:layout_marginRight="20dp"
        android:onClick="goVenueMap"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ICWS Info"
        android:id="@+id/textView_icwsInfo"
        android:textSize="12dp"
        android:textColor="#000000"
        android:typeface="normal"
        android:textStyle="bold"
        android:layout_below="@+id/imageButton_itinerary"
        android:layout_alignLeft="@+id/imageButton_icwsInfo"
        android:layout_alignStart="@+id/imageButton_icwsInfo" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:id="@+id/imageButton_icwsInfo"
        android:src="@mipmap/info"
        android:layout_above="@+id/textView_icwsInfo"
        android:layout_alignLeft="@+id/imageButton_event"
        android:layout_alignStart="@+id/imageButton_event"
        android:onClick="goICWSInfo"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Venue Map"
        android:id="@+id/textView_venueMap"
        android:textSize="12dp"
        android:textColor="#000000"
        android:typeface="normal"
        android:textStyle="bold"
        android:layout_alignTop="@+id/textView_cityMap"
        android:layout_alignRight="@+id/imageButton_venueMap"
        android:layout_alignEnd="@+id/imageButton_venueMap"
        android:layout_marginRight="-5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/browse_program"
        android:id="@+id/textView_program"
        android:textSize="12dp"
        android:textColor="#000000"
        android:typeface="normal"
        android:textStyle="bold"
        android:layout_above="@+id/imageButton_venueMap"
        android:layout_alignRight="@+id/imageButton_venueMap"
        android:layout_alignEnd="@+id/imageButton_venueMap"
        android:layout_marginBottom="20dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:id="@+id/imageButton_program"
        android:src="@mipmap/program"
        android:layout_above="@+id/textView_program"
        android:layout_alignRight="@+id/imageButton_venueMap"
        android:layout_alignEnd="@+id/imageButton_venueMap"
        android:onClick="goProgram"/>


</RelativeLayout>
